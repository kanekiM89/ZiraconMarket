</head>

{% if (column_left and column_right) %}
{% set layoutclass = 'layout-3' %}
{% elseif (column_left or column_right) %}
{% if column_left %}
{% set layoutclass = 'layout-2 left-col' %}
{% elseif column_right %}
{% set layoutclass = 'layout-2 right-col' %}
{% endif %}
{% else %}
{% set layoutclass = 'layout-1' %}
{% endif %}

<body class="{{ class }} {{ " " }} {{ layoutclass }}">

<header>
  
 
<div class="header" id="header">
	<div class="header-nav-wrapper">
		<div class="container">
			<div class="header-nav-left">
				<div class="nav pull-left">
				<div class="topmenu" title="{{ text_menu }}">{{ menu }}</div>
				</div>
			</div>
			<div class="header-nav-right">
			<div class="language-currency-block-inner">
						{{ language }}
						{{ currency }}
					</div>
				{{ header_top }}
			</div>
		</div>
	</div>

	<div class="content_headercms_top">
		<div class="container"> 
			{{ header_bottom }}
			
			
			<div class="header-logo">
				<div id="logo">{% if logo %}<a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>{% else %}
				<h1><a href="{{ home }}">{{ name }}</a></h1>
				{% endif %}</div>
			</div>
			
			
			
			<div class="header-cart">{{ cart }}</div>
			
			<div class="dropdown myaccount">
			
					<a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle userinfo" data-toggle="dropdown">
						<span class="user-info-image"></span>
						<span class="account-title">{{ text_account }}<!-- <i class="fa fa-angle-down"></i> --></span><span class="caret"></span>
					</a>	
					
					<!--<a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown"><!--<span class="account-title">{{ text_account }}</span><span class="caret"></span>
					<span class="user-info-image">{{ text_account }}<i class="fa fa-angle-down"></i></span>
					</a>-->
	
					<ul class="dropdown-menu dropdown-menu-right myaccount-menu">
						{% if logged %}
						<li><a href="{{ account }}" title="{{ text_account }}">{{ text_account }}</a></li>
						<li><a href="{{ order }}" title="{{ text_order }}">{{ text_order }}</a></li>
						<li><a href="{{ transaction }}" title="{{ text_transaction }}">{{ text_transaction }}</a></li>
						<li><a href="{{ download }}" title="{{ text_download }}">{{ text_download }}</a></li>
						<li><a href="{{ logout }}" title="{{ text_logout }}">{{ text_logout }}</a></li>
						{% else %}
						<li><a href="{{ register }}" title="{{ text_register }}">{{ text_register }}</a></li>
						<li><a href="{{ login }}" title="{{ text_login }}">{{ text_login }}</a></li>
						{% endif %}
						<nav id="top">
							<!-- <div class="container"> -->
								
								<div id="top-links" class="nav pull-right">
									<ul class="list-inline">
										{#  <li><a href="{{ contact }}"><i class="fa fa-phone"></i></a> <span class="hidden-xs hidden-sm hidden-md">{{ telephone }}</span></li> #}
										<!-- <li class="dropdown myaccount"><a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown"><span>{{ text_account }}</span> <span class="caret"></span></a>
										<ul class="dropdown-menu dropdown-menu-right myaccount-menu">
										{% if logged %}
										<li><a href="{{ account }}">{{ text_account }}</a></li>
										<li><a href="{{ order }}">{{ text_order }}</a></li>
										<li><a href="{{ transaction }}">{{ text_transaction }}</a></li>
										<li><a href="{{ download }}">{{ text_download }}</a></li>
										<li><a href="{{ logout }}">{{ text_logout }}</a></li>
										{% else %}
										<li><a href="{{ register }}">{{ text_register }}</a></li>
										<li><a href="{{ login }}">{{ text_login }}</a></li>
										{% endif %}
										</ul>
										</li> -->
										<li><a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}"><span class="wishlist">{{ text_wishlist }}</span></a></li>
										{# <li><a href="{{ shopping_cart }}" title="{{ text_shopping_cart }}"><i class="fa fa-shopping-cart"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_shopping_cart }}</span></a></li> #}
										<li><a href="{{ checkout }}" title="{{ text_checkout }}"><span class="checkout">{{ text_checkout }}</span></a></li>
									</ul>
								</div>
							
								<div class="nav pull-left"></div>
								
							<!-- </div> -->
						</nav>
					</ul>
				</div>
			
			    <div class="dropdown search" title="{{ text_search }}">{{ search }}</div>
		</div>
	</div>
	
	<div class="content_headercms_bottom">
		<div class="container">		
			
		</div>
	</div>

</div>

</header>
